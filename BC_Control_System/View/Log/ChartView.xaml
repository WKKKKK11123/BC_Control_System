<UserControl
    x:Class="BC_Control_System.View.Log.ChartView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:BC_Control_System.View.Log"
    xmlns:local1="clr-namespace:BC_Control_CustomControl;assembly=BC_Control_CustomControl"
    xmlns:local3="clr-namespace:BC_Control_Models.ClassK.SQLService;assembly=BC_Control_Models"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Grid>
        <local1:UserPlotControl
            Width="1600"
            Height="800"
            LinesType="{Binding ValueType}"
            ValueLines="{Binding CurveValues}" />
    </Grid>
</UserControl>
